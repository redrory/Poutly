<li id="<%= feed_item.id %>">
  <span class="user">
    <%= feed_item.name %>
    <%= feed_item.due_date %>
  </span>

  <span class="project_name"><%= feed_item.project_name %></span>
  <%= link_to "Delete", feed_item, method: :delete, 
  		data: {confirm: "You sure"}, title: feed_item.project_name  %>
  <span class="timestamp">
    Posted <%= time_ago_in_words(feed_item.created_at) %> ago.
  </span>
</li>